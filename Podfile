# Define a global platform for our project
platform :ios, '9.0'

source 'git@github.com:CocoaPods/Specs.git'
use_frameworks!	# needed to support Swift-based pods

target 'YapImageManagerExample' do

# normal pods (read-only)
  pod 'YapImageManager', :path => './'
	pod 'Alamofire', '4.4.0'
	pod 'AlamofireImage', '3.2.0'
	pod 'PromiseKit/CorePromise', '4.1.7'
	pod 'CocoaLumberjack/Swift', '3.1.0'
end
